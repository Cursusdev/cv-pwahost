{
  "name": "static-webpack",
  "version": "1.0.0",
  "description": "curriculum-vitae",
  "main": "entry.js",
  "scripts": {
    "start": "yarn prod && docker build -f Dockerfile -t cv-pwahost . && docker run --rm -d -p 8080:80/tcp cv-pwahost:latest",
    "dev": "yarn build && webpack serve --mode development --config webpack.dev.js",
    "prod": "yarn clean && set NODE_ENV=production && webpack --config webpack.prod.js",
    "build": "yarn clean && set NODE_ENV=development && webpack --config webpack.dev.js",
    "clean": "rm -rf dist",
    "deploy": "yarn prod && firebase deploy --debug --token \"$FIREBASE_TOKEN\"",
    "kill": "docker container rm -f $(docker ps --filter ancestor=cv-pwahost:latest -q) && docker rmi cv-pwahost"
  },
  "keywords": [],
  "author": "Y.Goalen",
  "license": "MIT",
  "devDependencies": {
    "base64-inline-loader": "^1.1.1",
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-loader": "^1.3.0",
    "html-loader-srcset": "^0.5.6",
    "html-srcsets-loader": "^0.5.1",
    "html-webpack-plugin": "^4.4.1",
    "image-webpack-loader": "^7.0.1",
    "imagemin": "^7.0.1",
    "imagemin-webp": "^6.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "offline-plugin": "^5.0.7",
    "popper.js": "^1.14.7",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^5.2.1",
    "svg-spritemap-webpack-plugin": "^3.8.2",
    "svg-url-loader": "^6.0.0",
    "url-loader": "^4.1.0",
    "webp-loader": "^0.6.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^4.6.0",
    "webpack-css-replace-images-to-webp": "^1.0.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "optimize-css-assets-webpack-plugin": "^5.0.4"
  }
}
